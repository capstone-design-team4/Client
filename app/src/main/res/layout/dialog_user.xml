<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="380dp"
    android:layout_height="600dp"
    android:background="@drawable/border_layout2">

    <TextView
        android:id="@+id/title_dialog_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/font1"
        android:text="세대 정보"
        android:textColor="#505050"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.051" />

    <TextView
        android:id="@+id/text_dialog_user1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/font1"
        android:text="이번 달 전기 요금"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_dialog_user"
        app:layout_constraintVertical_bias="0.215" />

    <TextView
        android:id="@+id/text_dialog_user2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/font1"
        android:text="현재 사용중인 전력원"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dialog_user1"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/text_dialog_user3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/font1"
        android:text="이번 달 동안 사용한 전력량"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dialog_user2"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/text_dialog_user5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/font1"
        android:text="하루동안 사용한 전력량"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dialog_user3"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/text_edit_dialog_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:fontFamily="@font/font1"
        android:text="30,000 원"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/text_dialog_user1"
        app:layout_constraintStart_toStartOf="@+id/text_dialog_user1"
        app:layout_constraintTop_toBottomOf="@+id/view_dialog_user"
        app:layout_constraintVertical_bias="0.215" />

    <TextView
        android:id="@+id/text_edit_dialog_user2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:fontFamily="@font/font1"
        android:text="발전 전력"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/text_dialog_user1"
        app:layout_constraintStart_toStartOf="@+id/text_dialog_user1"
        app:layout_constraintTop_toBottomOf="@+id/text_edit_dialog_user"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/text_edit_dialog_user3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:fontFamily="@font/font1"
        android:text="12345 kWh"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/text_dialog_user1"
        app:layout_constraintStart_toStartOf="@+id/text_dialog_user1"
        app:layout_constraintTop_toBottomOf="@+id/text_edit_dialog_user2"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/text_edit_dialog_user4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:fontFamily="@font/font1"
        android:text="1234 kWh"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/text_dialog_user1"
        app:layout_constraintStart_toStartOf="@+id/text_dialog_user1"
        app:layout_constraintTop_toBottomOf="@+id/text_edit_dialog_user3"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/text_start_graph"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:fontFamily="@font/font1"
        android:text="그래프로 보러 가기 >>"
        android:textColor="#505050"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/text_dialog_user1"
        app:layout_constraintStart_toStartOf="@+id/text_dialog_user1"
        app:layout_constraintTop_toBottomOf="@+id/text_edit_dialog_user4"
        app:layout_constraintVertical_bias="0.15" />

    <View
        android:id="@+id/view_dialog_user"
        android:layout_width="360dp"
        android:layout_height="2dp"
        android:layout_gravity="center_horizontal"
        android:background="@color/gray_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_dialog_user"
        app:layout_constraintVertical_bias="0.028"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>